{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "secrets.js",
      "node_modules"
    ],
    "redirects": [ {
      "source" : "/callback",
      "destination" : "https://us-central1-votify-b9360.cloudfunctions.net/callback",
      "type" : 301
    }],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
