{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "secrets.js",
      "node_modules",
      "serviceAccounts.json"
    ],
    "redirects": [
      {
        "source": ''
      }
    ]
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
